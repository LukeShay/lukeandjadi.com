{
  "name": "luke-jadi-wedding",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "db:migrate:make": "knex migrate:make",
    "db:migrate": "knex migrate:latest",
    "db:migrate:local": "dotenv -e .env.local -- yarn db:migrate --env local",
    "db:migrate:dev": "dotenv -e .env.dev -- yarn db:migrate --env development",
    "db:migrate:prod": "dotenv -e .env.prod -- yarn db:migrate --env production"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "next": "latest",
    "nodemailer": "^6.6.0",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@tailwindcss/jit": "0.1.18",
    "@types/cookie": "^0.4.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.0.1",
    "@types/nodemailer": "^6.4.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
